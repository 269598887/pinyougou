<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	   http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd
      ">

    <dubbo:protocol name="dubbo" port="20888"></dubbo:protocol>
    <dubbo:application name="pinyougou_cart_service"/>
    <dubbo:registry address="zookeeper://192.168.25.134:2181"/>
    <dubbo:annotation package="com.pinyougou.order.servive.impl"/>

    <bean id="idWorker" class="com.pinyougou.utlis.IdWorker">
        <!-- 进程 ID -->
        <constructor-arg index="0" value="0"></constructor-arg>
        <!-- 数据中心 ID -->
        <constructor-arg index="1" value="0"></constructor-arg>
    </bean>
</beans>